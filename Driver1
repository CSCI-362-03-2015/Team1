#include <stdio.h>
#include <stdlib.h>
#include <unistd.h>


//#include "/TestAutomation/tor-0.2.6.10/src/common/util.c"


//copy pasted from util.c
unsigned
round_to_next_multiple_of(unsigned number, unsigned divisor)
{
  number += divisor - 1;
  number -= number % divisor;
  return number;
}



int main(void){
    
    unsigned a = 13;
    unsigned b = 4;
    
    unsigned num2round;
    num2round = round_to_next_multiple_of(a, b);
    printf("here is result:%d", num2round);
    printf("\n");
 
    //writing to file, fopen creates it as well
    chdir("cd ..");
    chdir("cd ../temp");
    FILE *file;
    file = fopen("TestCase1.txt", "w");
    fprintf(file, "%d", num2round);
    fclose(file);

    
}
